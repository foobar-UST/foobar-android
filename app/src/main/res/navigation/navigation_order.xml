<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_order"
    app:startDestination="@id/orderFragment">

    <fragment
        android:id="@+id/orderFragment"
        android:name="com.foobarust.android.order.OrderFragment"
        android:label="@string/order_label"
        tools:layout="@layout/fragment_order">
        <deepLink app:uri="foobarust://navigation_order" />
    </fragment>

    <dialog
        android:id="@+id/cartFragment"
        android:name="com.foobarust.android.cart.CartFragment"
        android:label="@string/cart_label"
        tools:layout="@layout/fragment_cart"/>

    <action
        android:id="@+id/action_global_cartFragment"
        app:destination="@id/cartFragment"/>


</navigation>