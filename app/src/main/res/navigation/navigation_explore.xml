<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_explore"
    app:startDestination="@id/exploreFragment">

    <fragment
        android:id="@+id/exploreFragment"
        android:name="com.foobarust.android.explore.ExploreFragment"
        android:label="@string/fragment_label_explore"
        tools:layout="@layout/fragment_explore">
        <deepLink app:uri="foobarust://navigation_explore" />
    </fragment>

    <dialog
        android:id="@+id/cartFragment"
        android:name="com.foobarust.android.cart.CartFragment"
        tools:layout="@layout/fragment_cart"/>

    <action
        android:id="@+id/action_global_cartFragment"
        app:destination="@id/cartFragment"/>


</navigation>